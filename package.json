{
  "name": "@saavn-labs/sdk",
  "version": "0.1.0",
  "description": "A low-level, type-safe TypeScript SDK over JioSaavnâ€™s native API, built for developers who want full control, predictable contracts, and long-term stability.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": null
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "test": "vitest",
    "test:run": "vitest run",
    "examples:api": "bun examples/api/server.ts",
    "prettier": "prettier --write ."
  },
  "keywords": [
    "saavn-labs",
    "jiosaavn",
    "saavn",
    "music",
    "api",
    "sdk",
    "typescript",
    "cloudflare-workers",
    "vercel-serverless",
    "vercel-edge",
    "bun",
    "deno",
    "streaming",
    "download",
    "songs",
    "albums",
    "artists",
    "playlists",
    "trending"
  ],
  "author": "Saavn Labs <saavnlabs@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saavn-labs/sdk.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/saavn-labs/sdk/issues"
  },
  "homepage": "https://github.com/saavn-labs/sdk#readme",
  "dependencies": {
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.7.4",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18"
  }
}
